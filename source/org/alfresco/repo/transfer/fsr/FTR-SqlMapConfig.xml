<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>

    <settings
            useStatementNamespaces="true"
            forceMultipleResultSetSupport="false"
            lazyLoadingEnabled="false"
            enhancementEnabled="false"
            maxTransactions="500"
            maxSessions="500"
    />

    <!-- transactionManager type="EXTERNAL">
            <dataSource type="DBCP">
            </dataSource>
    </transactionManager -->

    <typeHandler javaType="java.io.Serializable" jdbcType="BLOB" callback="org.alfresco.ibatis.SerializableTypeHandlerCallback"/>


    <sqlMap resource="org/alfresco/repo/transfer/fsr/qname-common-SqlMap.xml"/>
    <sqlMap resource="org/alfresco/repo/transfer/fsr/qname-insert-SqlMap.xml"/>
    <sqlMap resource="org/alfresco/repo/transfer/fsr/locks-common-SqlMap.xml"/>
    <sqlMap resource="org/alfresco/repo/transfer/fsr/locks-insert-SqlMap.xml"/>

</sqlMapConfig>